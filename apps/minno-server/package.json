{
  "name": "@cyrus/minno-server",
  "version": "0.1.0",
  "description": "Minno server for Slack/Notion integration with Sevalla deployment",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "db:init": "tsx scripts/init-db.ts",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:seed": "tsx scripts/seed.ts"
  },
  "keywords": [
    "slack",
    "notion",
    "express",
    "cyrus"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.0",
    "@slack/web-api": "^7.0.0",
    "@slack/events-api": "^3.0.1",
    "@slack/oauth": "^2.6.0",
    "pg": "^8.11.0",
    "dotenv": "^16.0.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "cyrus-slack-client": "workspace:*",
    "cyrus-edge-worker": "workspace:*",
    "cyrus-core": "workspace:*"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/cors": "^2.8.0",
    "@types/morgan": "^1.9.0",
    "@types/pg": "^8.11.0",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0",
    "vitest": "^1.2.0"
  }
}
